{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/background_image_formatter": "^1.3",
        "drupal/bg_image_formatter": "^1.1",
        "drupal/bootstrap": "^3.11",
        "drupal/bootstrap4": "^1.0@beta",
        "drupal/colorbox": "^1.4",
        "drupal/colordialog": "^1.1",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6.0",
        "drupal/cors": "^1.0",
        "drupal/ctools": "^3.0",
        "drupal/date_range_formatter": "^3.1",
        "drupal/devel": "^1.2",
        "drupal/ds": "^3.1",
        "drupal/entity_browser": "^1.4",
        "drupal/entity_print": "^2.0",
        "drupal/exclude_node_title": "^1.0@beta",
        "drupal/field_group": "^1.0",
        "drupal/fontawesome": "^2.2",
        "drupal/iframe": "^1.9",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jsonapi": "^1.23",
        "drupal/jsonapi_file": "^1.1",
        "drupal/menu_attributes": "1.x-dev",
        "drupal/menus_attribute": "^1.1",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.3",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.2",
        "drupal/printable": "^1.0",
        "drupal/responsive_menu": "^2.6",
        "drupal/restui": "^1.15",
        "drupal/taxonomy_menu": "^3.3",
        "drupal/text_resize": "^1.1",
        "drupal/video": "^1.2",
        "drupal/video_embed_field": "^2.0",
        "drupal/videojs": "^1.0",
        "drupal/views_bootstrap": "^3.0",
        "drupal/views_slideshow": "^4.6",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        }
    }
}
